{
  "name": "react-minimal-template",
  "version": "1.0.0",
  "description": "Minimal template project for experiments with ReactJS",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanceon/react-minimal-template.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "AZ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lanceon/react-minimal-template/issues"
  },
  "homepage": "https://github.com/lanceon/react-minimal-template#readme",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --progress --colors --hot --port 8080",
    "build": "cross-env BABEL_ENV=production webpack -p --config webpack.prod.js --progress --display-modules --display-chunks",
    "server": "webpack-dev-server --config server.js",
    "prod": "yarn build && yarn server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-devtools": "^3.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "engines": {
    "node": ">=6.9.1",
    "yarn": ">=0.20.3"
  }
}
